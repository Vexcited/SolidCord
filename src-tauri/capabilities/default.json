{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "websocket:default",
    "shell:allow-open",
    "os:allow-os-type",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://discord.com/**" }, { "url": "https://cordapi.dolfi.es/api/v2/properties/*" }]
    }
  ]
}
